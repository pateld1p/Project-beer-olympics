{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        },
        {
            "src": "api/server.js",
            "use": "@vercel/node"  
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "api/server.js" }  
    ]
}
